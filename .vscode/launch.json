{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: article_script.py (test_spn)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/article_script.py",
            "console": "integratedTerminal",
            "args": ["test_inputs"],
            "cwd": "${workspaceFolder}",
            "justMyCode": true
        },
        {
            "name":"Python: benchmark.py with iris",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/benchmark.py",
            "console": "integratedTerminal",
            "args": ["-d", "iris", "-m", "MP", "AMP", "MS", "HBP", "--no-learn"],
            "cwd": "${workspaceFolder}",
            "justMyCode": true
        },
        {
            "name":"Python: benchmark.py with audiology",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/benchmark.py",
            "console": "integratedTerminal",
            "args": ["-d", "audiology", "-m", "MP", "AMP", "MS", "HBP", "--no-learn", "--data-path", "benchmark_datasets"],
            "cwd": "${workspaceFolder}",
            "justMyCode": true
        },
        {
            "name":"Python: benchmark.py all datasets",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/benchmark.py",
            "console": "integratedTerminal",
            "args": ["-m", "MP", "AMP", "MS", "HBP", "--no-learn", "-all", "--data-path", "benchmark_datasets"],
            "cwd": "${workspaceFolder}",
            "justMyCode": true
        }
    ]
}