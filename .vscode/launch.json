{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: article_script.py (test_spn)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/article_script.py",
            "console": "integratedTerminal",
            "args": ["test_inputs"],
            "cwd": "${workspaceFolder}",
            "justMyCode": true
        },
        {
            "name":"Python: benchmark.py with iris",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/benchmark.py",
            "console": "integratedTerminal",
            "args": ["-d", "iris", "-m", "MP", "AMP", "MS", "HBP", "--no-learn"],
            "cwd": "${workspaceFolder}",
            "justMyCode": true
        },
        {
            "name":"Python: benchmark.py with audiology",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/benchmark.py",
            "console": "integratedTerminal",
            "args": ["-d", "audiology", "-m", "MP", "AMP", "MS", "HBP", "--no-learn", "--data-path", "benchmark_datasets"],
            "cwd": "${workspaceFolder}",
            "justMyCode": true
        },
        {
            "name":"Python: benchmark.py all datasets",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/benchmark.py",
            "console": "integratedTerminal",
            "args": ["-m", "MP", "AMP", "MS", "HBP", "--no-learn", "-all", "--data-path", "20-datasets", "-q", "0.4", "-e", "0.4"],
            "cwd": "${workspaceFolder}",
            "justMyCode": true
        }, 
        {
            "name":"Python: make_queries&evids.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/make_queries&evids.py",
            "console": "integratedTerminal",
            "args": ["-dp", "20-datasets", "-d", "accidents"],
            "cwd": "${workspaceFolder}",
            "justMyCode": true
        },
        {
            "name":"Python: spn_to_json.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/spn_to_json.py",
            "console": "integratedTerminal",
            "args": ["test_inputs/iris/iris.spn"],
            "cwd": "${workspaceFolder}",
            "justMyCode": true
        },
                {
            "name":"Python: plot_results.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/plot_results.py",
            "console": "integratedTerminal",
            "args": ["-q", "0.1", "-e", "0.1", "-dn", "small_datasets", "-dt", "2025-11-05 20-02-33"],
            "cwd": "${workspaceFolder}",
            "justMyCode": true
